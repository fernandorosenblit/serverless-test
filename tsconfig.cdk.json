{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "lib": ["es2018", "dom"],
    "declaration": false,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "typeRoots": ["./node_modules/@types"]
  },
  "exclude": ["cdk.out", "aws"],
  "baseUrl": "./",
  "paths": {
    "*": ["src/*"],
    "httpClient/*": ["src/httpClient/*"],
    "components/*": ["src/components/*"],
    "assets/*": ["src/assets/*"],
    "middleware/*": ["src/middleware/*"],
    "constants/*": ["src/constants/*"],
    "cypressConstants": ["../cypress/support/constants"],
    "fixtures": ["../cypress/fixtures"],
    "hooks/*": ["src/hooks/*"],
    "locales/*": ["src/locales/*"],
    "pages/*": ["src/pages/*"],
    "state/*": ["src/state/*"],
    "reusableTests": ["../cypress/support/reusableTests"],
    "selectors/*": ["src/selectors/*"],
    "services/*": ["src/services/*"],
    "stubs": ["../cypress/stubs"],
    "styles/*": ["src/styles/*"],
    "utils/*": ["src/utils/*"]
  }
}
